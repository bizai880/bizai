{
  "name": "@bizai/web",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:legacy": "next dev",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "start:prod": "NODE_ENV=production next start",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "type-check": "tsc --noEmit --incremental",
    "type-check:watch": "tsc --noEmit --watch --preserveWatchOutput",
    "clean": "turbo clean && rm -rf .next .turbo node_modules/.cache",
    "clean:all": "rm -rf .next .turbo node_modules dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --coverage",
    "postinstall": "echo 'âœ… @bizai/web dependencies installed'",
    "vercel:deploy": "vercel --prod",
    "vercel:preview": "vercel",
    "vercel:env": "vercel env pull .env.local",
    "ignoreCommand": "cd ../../ && npm install --legacy-peer-deps"
  },
  "dependencies": {
    "@bizai/shared": "file:../../packages/shared",
    "@cloudinary/react": "1.14.3",
    "@cloudinary/url-gen": "1.22.0",
    "@supabase/supabase-js": "^2.44.0",
    "atob": "2.1.2",
    "autoprefixer": "^10.4.20",
    "btoa": "1.2.1",
    "exceljs": "^4.4.0",
    "framer-motion": "^11.0.0",
    "husky": "^9.1.7",
    "is-deep-strict-equal-x": "1.1.2",
    "jsonwebtoken": "^9.0.2",
    "lru-cache": "11.2.4",
    "lucide-react": "^0.562.0",
    "next": "15.5.9",
    "next-auth": "^4.24.7",
    "next-cloudinary": "6.17.5",
    "postcss": "^8.4.41",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "rimraf": "6.1.2",
    "tailwindcss": "^3.4.0",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "16.1.1",
    "@svgr/plugin-jsx": "^8.1.0",
    "@svgr/plugin-svgo": "^8.1.0",
    "@svgr/webpack": "^8.1.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/domexception": "4.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vercel/analytics": "^1.2.2",
    "@vercel/speed-insights": "^1.0.12",
    "eslint": "^9.39.2",
    "eslint-config-next": "16.1.1",
    "eslint-config-turbo": "^2.7.2",
    "eslint-plugin-jest": "^29.11.0",
    "eslint-plugin-testing-library": "^6.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "turbo": "^2.7.2"
  },
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=11.7.0"
  },
  "packageManager": "npm@11.7.0",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "turbo": {
    "pipeline": {
      "dev": {
        "dependsOn": [
          "^build"
        ],
        "cache": false,
        "persistent": true
      },
      "build": {
        "dependsOn": [
          "^build",
          "@bizai/shared#build"
        ],
        "outputs": [
          ".next/**",
          "dist/**"
        ]
      },
      "lint": {
        "outputs": []
      },
      "test": {
        "dependsOn": [
          "^build"
        ],
        "outputs": [
          "coverage/**"
        ]
      },
      "type-check": {
        "dependsOn": [
          "^type-check"
        ],
        "outputs": []
      },
      "clean": {
        "cache": false
      }
    }
  },
  "vercel": {
    "buildCommand": "npm run build",
    "outputDirectory": ".next",
    "devCommand": "npm run dev",
    "installCommand": "npm install --legacy-peer-deps",
    "framework": "nextjs",
    "git": {
      "deploymentEnabled": true,
      "autoAlias": true
    }
  },
  "publishConfig": {
    "access": "restricted",
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist",
    "src",
    "public",
    "next.config.js",
    "next.config.mjs",
    "postcss.config.js",
    "tailwind.config.js",
    "tsconfig.json"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "module": "esnext",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "resolutions": {
    "next": "15.5.9",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3"
  }
}
