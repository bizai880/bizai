{
	"name": "bizai-factory",
	"version": "1.0.0",
	"private": true,
	"type": "module",
	"packageManager": "npm@11.7.0",
	"scripts": {
		"dev": "turbo dev",
		"dev:web": "turbo dev --filter=./apps/web",
		"dev:ai-worker": "turbo dev --filter=./apps/ai-worker",
		"dev:turbopack": "turbo dev --filter=./apps/web -- --turbopack",
		"dev:modelhub": "turbo dev --filter=./apps/modelhub",
		"dev:all": "turbo dev --filter=./apps/web --filter=./apps/ai-worker --filter=./apps/modelhub",
		"build": "turbo build",
		"build:web": "turbo build --filter=./apps/web",
		"build:ai-worker": "turbo build --filter=./apps/ai-worker",
		"build:modelhub": "turbo build --filter=./apps/modelhub",
		"build:shared": "turbo build --filter=./packages/shared",
		"build:prod": "turbo build --mode=production",
		"start": "turbo start --filter=./apps/web",
		"start:prod": "NODE_ENV=production turbo start --filter=./apps/web",
		"start:modelhub": "turbo start --filter=./apps/modelhub",
		"lint": "biome lint .",
		"lint:fix": "biome lint --write .",
		"lint:staged": "biome check --staged --write",
		"lint:ci": "biome ci .",
		"lint:format": "biome format --write .",
		"lint:organize": "biome check --write --unsafe --organize-imports .",
		"format": "biome format --write .",
		"format:check": "biome format .",
		"format:staged": "biome format --staged --write",
		"check": "biome check .",
		"check:fix": "biome check --write --unsafe .",
		"check:ci": "biome ci .",
		"check:staged": "biome check --staged --write",
		"type-check": "turbo type-check",
		"type-check:watch": "turbo type-check -- --watch",
		"test": "turbo test",
		"test:watch": "turbo test -- --watch",
		"test:ci": "turbo test -- --coverage",
		"clean": "turbo clean && rm -rf node_modules/.cache",
		"clean:all": "turbo clean && rm -rf node_modules apps/*/node_modules packages/*/node_modules",
		"clean:install": "npm run clean:all && npm install",
		"clean:biome": "rm -rf .biome-cache",
		"install:all": "npm install && turbo run install",
		"install:ci": "npm ci --legacy-peer-deps",
		"biome": "biome check .",
		"biome:check": "biome check .",
		"biome:fix": "biome lint --write --unsafe .",
		"biome:format": "biome format --write .",
		"biome:ci": "biome ci .",
		"biome:init": "biome init",
		"biome:upgrade": "npm install @biomejs/biome@latest",
		"fix:all": "npm run lint:fix && npm run format",
		"docker:dev": "docker-compose -f docker-compose.dev.yml up -d",
		"docker:prod": "docker-compose -f infra/docker/docker-compose.prod.yml up -d",
		"docker:down": "docker-compose -f docker-compose.dev.yml down",
		"docker:modelhub": "cd apps/modelhub && npm run docker:build",
		"vercel:deploy": "cd apps/web && vercel --prod",
		"vercel:preview": "cd apps/web && vervel",
		"vercel:env": "cd apps/web && vercel env pull .env.local",
		"vercel:link": "cd apps/web && vercel link",
		"health": "turbo run type-check lint build --dry-run",
		"prepare": "husky install || echo 'Husky not installed'",
		"postinstall": "npm run prepare",
		"cubes:generate": "cd apps/modelhub && npm run generate-cube",
		"cubes:test": "cd apps/modelhub && npm test",
		"precommit": "npm run lint:staged",
		"prepush": "npm run lint && npm run type-check && npm run test"
	},
	"workspaces": [
		"apps/*",
		"packages/*"
	],
	"dependencies": {
		"biome": "^0.3.3",
		"ci": "^2.3.0",
		"turbo": "^2.7.2"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.10",
		"@types/node": "^22.0.0",
		"@typescript-eslint/eslint-plugin": "^8.0.0",
		"@typescript-eslint/parser": "^8.0.0",
		"eslint": "^9.39.2",
		"eslint-config-prettier": "^9.1.0",
		"eslint-config-turbo": "^2.0.0",
		"eslint-plugin-prettier": "^5.2.1",
		"husky": "^9.1.7",
		"lint-staged": "^15.2.9",
		"prettier": "^3.3.3",
		"typescript": "^5.9.3"
	},
	"peerDependencies": {
		"node": "^22.0.0",
		"npm": ">=11.7.0"
	},
	"engines": {
		"node": ">=v22.0.0",
		"npm": ">=11.7.0"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"turbo": {
		"tasks": {
			"build": {
				"dependsOn": [
					"^build"
				],
				"outputs": [
					".next/**",
					"dist/**",
					"!dist/.next/**"
				]
			},
			"dev": {
				"cache": false,
				"persistent": true
			},
			"lint": {
				"outputs": []
			},
			"type-check": {
				"outputs": []
			},
			"test": {
				"outputs": [
					"coverage/**"
				]
			},
			"clean": {
				"cache": false
			}
		},
		"globalDependencies": [
			"**/.env.*",
			"**/.env.local",
			"turbo.json",
			"package.json",
			"biome.json"
		],
		"globalEnv": [
			"CI",
			"NODE_ENV",
			"VERCEL",
			"GITPOD_WORKSPACE_URL",
			"ONA_API_KEY",
			"BIOME_LOG_LEVEL"
		]
	},
	"ona": {
		"projectType": "turborepo",
		"automations": {
			"default": [
				"turbo-fix-ai",
				"security-audit-ai"
			],
			"ci": [
				"turbo-performance-ai"
			]
		},
		"environment": {
			"nodeVersion": "22",
			"turboVersion": "2.7.2",
			"cacheStrategy": "distributed",
			"biomeVersion": "2.3.10"
		}
	},
	"vercel": {
		"scope": "bizai",
		"git": {
			"deploymentEnabled": true,
			"autoAlias": true
		},
		"projects": {
			"web": {
				"path": "apps/web"
			}
		}
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx,json,css,md,vue,svelte,astro}": [
			"biome check --write --no-errors-on-unmatched"
		]
	},
	"publishConfig": {
		"access": "restricted",
		"registry": "https://registry.npmjs.org/"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/bizai8080/bizai.git"
	},
	"keywords": [
		"turborepo",
		"nextjs",
		"turbopack",
		"monorepo",
		"typescript",
		"biome",
		"ai",
		"modelhub"
	],
	"author": "BizAI Team",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/bizai8080/bizai/issues"
	},
	"homepage": "https://bizai.dev",
	"files": [
		"apps/",
		"packages/",
		"scripts/",
		"infra/",
		"*.json",
		"*.md"
	],
	"resolutions": {
		"turbo": "^2.7.2",
		"next": "15.5.9",
		"react": "^19.2.7",
		"react-dom": "^19.2.3",
		"@typescript-eslint/eslint-plugin": "^8.0.0",
		"@typescript-eslint/parser": "^8.0.0",
		"eslint": "^9.39.2",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.2.1",
		"prettier": "^3.3.3",
		"@biomejs/biome": "^2.3.10"
	},
	"biome": {
		"extends": "./biome.json"
	}
}
